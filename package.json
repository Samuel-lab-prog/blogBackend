{
	"name": "blogback",
	"module": "index.ts",
	"type": "module",
	"private": true,
	"devDependencies": {
		"@elysiajs/cors": "^1.4.0",
		"@elysiajs/openapi": "^1.4.11",
		"@eslint/js": "^9.38.0",
		"@types/bun": "latest",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^24.10.1",
		"@types/pg": "^8.15.6",
		"barrelsby": "^2.8.1",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"globals": "^16.4.0",
		"jiti": "^2.6.1",
		"prisma": "^7.0.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.2"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"scripts": {
		"build": "bun build src/server.ts --compile --minify-whitespace --minify-syntax --outfile dist/server",
		"start": "./dist/server",
		"generate": "npx prisma generate",
		"migrateprod": "npx prisma migrate deploy",
		"migratedev": "npx prisma migrate dev",
		"dev": "bun --watch src/server.ts",
		"lint": "eslint . ",
		"lint:fix": "eslint . --fix",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"test": "bun test",
		"seed": "npx prisma db seed",
		"reset": "npx prisma migrate reset --force",
		"drop": "npx prisma db push --force-reset",
		"barrels": "barrelsby --config barrels.config.json"
	},
	"dependencies": {
		"@prisma/adapter-pg": "^7.2.0",
		"@prisma/client": "^7.2.0",
		"bcryptjs": "^3.0.3",
		"dotenv": "^17.2.3",
		"elysia": "^1.4.13",
		"elysia-rate-limit": "^4.4.2",
		"jsonwebtoken": "^9.0.2",
		"pino": "^10.1.0",
		"pino-pretty": "^13.1.3",
		"slugify": "^1.6.6",
		"xss": "^1.0.15"
	}
